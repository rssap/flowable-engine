<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:flowable="http://flowable.org/bpmn"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" exporter="edoras vis" exporterVersion="DEVELOPER"
             targetNamespace="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL http://www.omg.org/spec/BPMN/2.0/20100501/BPMN20.xsd">
  <process id="callActivity" isExecutable="true" name="call activity">
    <startEvent id="startevent1"/>

    <callActivity id="callActivity1" calledElement="calledProcessId" flowable:completeAsync="true">
      <multiInstanceLoopCharacteristics isSequential="false">
        <extensionElements>
          <flowable:variableAggregation target="reviews">
            <variable source="source" targetExpression="${nonExistingVariable}"/>
          </flowable:variableAggregation>
        </extensionElements>
        <loopCardinality>3</loopCardinality>
      </multiInstanceLoopCharacteristics>
    </callActivity>

    <endEvent id="noneevent1"/>

    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="callActivity1"/>

    <sequenceFlow id="flow4" sourceRef="callActivity1" targetRef="noneevent1"/>

  </process>
</definitions>